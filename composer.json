{
    "name": "mimmi20/wurflcache",
    "description": "the Cache Classes for the Wurfl PHP Library for PHP 5.3",
    "type": "library",
    "license": "LGPL-2.1",
    "keywords": ["Wurfl", "Cache"],
    "homepage": "https://github.com/mimmi20/WurflCache",
    "require": {
        "php": ">=5.3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "ext-xdebug": "*",
        "zendframework/zend-cache": "2.2.*",
        "desarrolla2/cache": "dev-master",
        "doctrine/cache": "1.4.*@dev",
        "zetacomponents/cache": "dev-master"
    },
    "suggest": {
        "zendframework/zend-cache": "to use other caches"
    },
    "autoload": {
        "psr-0": {
            "WurflCache\\": "library/"
        }
    }
}
